      "      <system value=\"${1|http://snomed.info/sct,http://terminology.hl7.org/CodeSystem/v3-RoleCode|}\"/>",
      "      <code value=\"TODO\"/>",
      "      <display value=\"TODO\"/>",
      "    </coding>",
      "  </relationship>",
      "  <name/>",
      "  <telecom/>",
      "  <gender value=\"${2|male,female,other,unknown|}\"/>",
      "  <birthDate value=\"${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}\"/>",
